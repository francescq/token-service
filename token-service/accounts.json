{
    "apiVersion": "1.0",
    "swaggerVersion": "1.1",
    "basePath":"http://localhost:1234/1.0",
    "resourcePath": "/accounts",
    "apis": [
        {
            "path": "/accounts.{format}",
            "description": "Management endpoint for all the accounts",
            "operations": [
                {
                    "parameters": [
                        {
                            "name": "sid",
                            "description": "A valid session id (you can use the cookie or basic auth as alternative methods to authenticate)",
                            "required": false,
                            "dataType": "string",
                            "allowMultiple": false,
                            "paramType": "query"
                        },
                        {
                            "name": "f",
                            "description": "Filter element that defines from which index you want the list (default 0)",
                            "required": false,
                            "dataType": "long",
                            "allowMultiple": false,
                            "paramType": "query"
                        },
                        {
                            "name": "n",
                            "description": "Filter element that defines how many items you want the list (default is sector, no limits)",
                            "required": false,
                            "dataType": "long",
                            "allowMultiple": false,
                            "paramType": "query"
                        },
                        {
                            "name": "q",
                            "description": "Filter element that defines a text query to be executed on the accounts (default is no query, get me everything)",
                            "required": false,
                            "dataType": "long",
                            "allowMultiple": false,
                            "paramType": "query"
                        }
                    ],
                    "summary": "Return a list of all the accounts as defined by the filter",
                    "httpMethod": "GET",
                    "errorResponses": [
                        {
                            "reason": "Account list not available (at least to you).",
                            "code": 403
                        }
                    ],
                    "nickname": "getAccounts",
                    "responseClass": "List[Account]"
                },
                {
                    "parameters": [
                        {
                            "name": "sid",
                            "description": "A valid session id (you can use the cookie or basic auth as alternative methods to authenticate)",
                            "required": false,
                            "dataType": "string",
                            "allowMultiple": false,
                            "paramType": "query"
                        },
                        {
                            "name": "account",
                            "description": "The proposed new account",
                            "required": true,
                            "dataType": "Account",
                            "allowMultiple": false,
                            "paramType": "body"
                        }
                    ],
                    "summary": "Creates a new account",
                    "httpMethod": "POST",
                    "errorResponses": [
                        {
                            "reason": "Invalid session",
                            "code": 403
                        },
                        {
                            "reason": "Account already taken found.",
                            "code": 404
                        }
                    ],
                    "nickname": "createAccount",
                    "responseClass": "Account"
                }
            ]
        },
        {
            "path": "/accounts/{id}",
            "description": "Management endpoint for a specific account",
            "operations": [
                {
                    "parameters": [
                        {
                            "name": "sid",
                            "description": "A valid session id (you can use the cookie or basic auth as alternative methods to authenticate)",
                            "required": false,
                            "dataType": "string",
                            "allowMultiple": false,
                            "paramType": "query"
                        },
                        {
                            "name": "aid",
                            "description": "A valid account id",
                            "required": true,
                            "dataType": "string",
                            "allowMultiple": false,
                            "paramType": "path"
                        }
                    ],
                    "summary": "Returns the full information of an account",
                    "httpMethod": "GET",
                    "errorResponses": [
                        {
                            "reason": "Invalid session",
                            "code": 403
                        },
                        {
                            "reason": "Account not found.",
                            "code": 404
                        }
                    ],
                    "nickname": "getAccount",
                    "responseClass": "Account",
                    "notes": "You may need extra powers to get information about accounts."
                }
            ]
        },
        {
            "path": "/accounts/{id}",
            "description": "Management endpoint for a specific account",
            "operations": [
                {
                    "parameters": [
                        {
                            "name": "sid",
                            "description": "A valid session id (you can use the cookie or basic auth as alternative methods to authenticate)",
                            "required": false,
                            "dataType": "string",
                            "allowMultiple": false,
                            "paramType": "query"
                        },
                        {
                            "name": "aid",
                            "description": "A valid account id",
                            "required": true,
                            "dataType": "string",
                            "allowMultiple": false,
                            "paramType": "path"
                        }
                    ],
                    "summary": "Removes an account",
                    "httpMethod": "DELETE",
                    "errorResponses": [
                        {
                            "reason": "Invalid session",
                            "code": 403
                        },
                        {
                            "reason": "Account not found.",
                            "code": 404
                        }
                    ],
                    "nickname": "removeAccount",
                    "responseClass": "Account",
                    "notes": "You may need extra powers to get information about accounts."
                }
            ]
        }
    ],
    "models": {
        "Account": {
            "properties": {
                "id": {
                    "type": "long"
                },
                "username": {
                    "type": "string"
                },
                "email": {
                    "type": "string"
                },
                "status": {
                    "type": "int"
                },
                "name": {
                    "type": "string"
                },
                "surname": {
                    "type": "string"
                },
                "password": {
                    "type": "string"
                }
            },
            "id": "Account"
        }
    }
}